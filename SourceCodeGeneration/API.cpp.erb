<%
states = system_ctrl_stm.states
events = system_ctrl_stm.events
module_name = system_ctrl_stm.module_name %>
#include "<%=module_name%>_API.h"

<%
def PrameterList(param_types)
    para_list = ""
    param_types.each do |type|
        if para_list == ""
            para_list = "%s _%s" % [type, type.underscore]
        else 
            para_list = "%s, %s _%s" % [para_list, type, type.underscore]
        end
    end
    para_list
end
%>

<% 
events.each do |event| %>
/* @brief Send <%=event.name%> message to <%=module_name%>
 *
 */
void Send<%=event.name%>MsgTo<%=module_name%>(<%=PrameterList(event.params.keys)%>) {

}
<%
end %>

