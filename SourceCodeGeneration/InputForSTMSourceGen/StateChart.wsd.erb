<%
states = module_stm.states
events = module_stm.events
module_name = module_stm.module_name %>

@startuml <%= module_name %>'s state chart

[*] --> Unitialized
<% 
module_stm.transitions.each do |tran| 
    if tran.s_state != tran.e_state
%>
<%= tran.s_state %> --> <%= tran.e_state %> : <%= tran.event %><%  
    end
end %>

@enduml


       