@startuml

<%
def generate_send_message_method(send_msg_method)
  params = send_msg_method.params.map {|param| "%s %s" % param}.join(", ")
  "SystemCtrlOpResult %s(%s)" % [send_msg_method.method_name, params]
end
%>

class <%= class_diag.module_name %> {<%
class_diag.send_message_methods.each do |method| %>
    + <%= generate_send_message_method(method) %><%
end%>
    - uint32 state
}


@enduml